{
  "name": "@genesys-ui/icons",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "pnpm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "pnpm run generate:font && pnpm run generate:js",
    "generate:font": "tsx ./scripts/font/generate.ts",
    "generate:font:ci": "tsx ./scripts/font/generate.ts --ci",
    "generate:react": "tsx ./scripts/react/generate.ts",
    "generate:js": "tsx ./scripts/js/generate.ts",
    "validations": "pnpm run validations:iconName",
    "validations:iconName": "tsx ./scripts/validations/iconName/checkIconName.ts",
    "lint": "oxlint -c oxlintrc.json",
    "tsup": "tsup",
    "clean": "rm -rf dist src/icons/GI* src/icons/index.ts",
    "test": "vitest",
    "build": "pnpm run clean && pnpm run generate:react && pnpm run tsup && pnpm run generate"
  },
  "devDependencies": {
    "@floating-ui/react": "^0.27.16",
    "@release-it/conventional-changelog": "^10.0.1",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/addon-themes": "^9.1.3",
    "@storybook/builder-vite": "^9.1.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react-vite": "^9.1.3",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/typescript-checker": "^9.0.1",
    "@stryker-mutator/vitest-runner": "^9.0.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitest/coverage-v8": "^3.2.4",
    "ahooks": "^3.9.4",
    "cheerio": "^1.1.2",
    "change-case": "^5.4.4",
    "decamelize": "^6.0.1",
    "domhandler": "^5.0.3",
    "happy-dom": "^18.0.1",
    "oxlint": "^1.12.0",
    "prettier": "^3.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.1.1",
    "react-window": "^1.8.11",
    "release-it": "^19.0.4",
    "sass": "^1.90.0",
    "storybook": "^9.1.3",
    "styled-components": "^6.1.19",
    "svgtofont": "^6.4.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/genesys-ui/icons.git"
  },
  "optionalDependencies": {
    "react": "19.x"
  }
}
